<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xjs.mapper.ApiWarningMapper">

    <update id="AllHaveRead">
        update api_warning
        set handle = 1
        where handle = 2
    </update>


    <delete id="clearAll">
        delete
        from api_warning
        where handle = 1
    </delete>
</mapper>